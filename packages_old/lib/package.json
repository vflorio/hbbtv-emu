{
  "name": "@hbb-emu/lib",
  "version": "1.0.0",
  "devDependencies": {
    "@types/chrome": "0.1.29",
    "fp-ts": "2.16.11",
    "io-ts": "2.2.22",
    "typescript": "5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "generate-exports": "npx tsx scripts/generate-exports.ts"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "app": [
        "./dist/app.d.ts"
      ]
    }
  }
}
